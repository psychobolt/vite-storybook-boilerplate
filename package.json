{
  "name": "vite-storybook-boilerplate",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "postinstall": "husky install",
    "g:dotenv": "cd $INIT_CWD && dotenv -e $PROJECT_CWD/.env -e $INIT_CWD/.env -c",
    "g:dotenv-vault": "cd $INIT_CWD && dotenv-vault",
    "g:swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
    "g:run-script": "cd $INIT_CWD && yarn g:dotenv -- node --loader file://$(yarn g:swc-register-path)",
    "run-script": "yarn g:run-script",
    "turbo": "yarn run-script scripts/turbo.ts",
    "ls-workspaces": "yarn run-script scripts/ls-workspaces.ts",
    "bootstrap": "yarn workspace commons turbo run build && yarn run-script ./scripts/bootstrap.ts",
    "apply-versions": "yarn run-script scripts/apply-versions.ts",
    "dev": "yarn turbo --background run dev",
    "build": "yarn turbo run build",
    "config-ci": "yarn run-script scripts/config-ci.ts",
    "lint": "yarn g:prettier --check . && yarn g:lint-js",
    "g:lint-css": "cd $INIT_CWD && stylelint --ignore-path $PROJECT_CWD/.gitignore \"**/*.{sc,c}ss\"",
    "format": "yarn g:prettier --write ."
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@swc-node/register": "1.9.0",
    "@swc/core": "1.4.8",
    "@types/glob-to-regexp": "0.4.4",
    "@types/node": "20.11.28",
    "@types/semver": "7.5.8",
    "@yarnpkg/core": "4.0.3",
    "@yarnpkg/fslib": "3.0.2",
    "@yarnpkg/sdks": "3.1.0",
    "arg": "5.0.2",
    "commons": "workspace:*",
    "dotenv-cli": "7.3.0",
    "dotenv-flow": "4.1.0",
    "dotenv-vault": "1.26.1",
    "eslint": "8.57.0",
    "execa": "8.0.1",
    "glob-to-regexp": "0.4.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "playwright": "1.42.1",
    "semver": "7.6.0",
    "stylelint": "15.11.0",
    "turbo": "1.12.5",
    "typescript": "5.4.2",
    "yaml": "2.4.1"
  },
  "resolutions": {
    "@swc-node/register": "patch:@swc-node/register@npm%3A1.9.0#~/.yarn/patches/@swc-node-register-npm-1.9.0-4530442723.patch",
    "dotenv-cli@7.3.0": "patch:dotenv-cli@npm%3A7.3.0#./.yarn/patches/dotenv-cli-npm-7.3.0-7b0ff066d1.patch",
    "dotenv-flow@4.1.0": "patch:dotenv-flow@npm%3A4.1.0#~/.yarn/patches/dotenv-flow-npm-4.1.0-74ebe7c968.patch",
    "dotenv-vault@1.26.1": "patch:dotenv-vault@npm%3A1.26.1#~/.yarn/patches/dotenv-vault-npm-1.26.1-f283958789.patch",
    "dotenv-vault/@oclif/core@^1": "npm:3.23.0",
    "jest-config@npm:^29.7.0": "patch:jest-config@npm%3A29.7.0#~/.yarn/patches/jest-config-npm-29.7.0-97d8544d74.patch"
  }
}
