{
  "name": "vite-storybook-boilerplate",
  "private": true,
  "type": "module",
  "packageManager": "yarn@3.5.0",
  "scripts": {
    "ls-workspaces": "yarn node --loader file://$(yarn node -p \"require.resolve('ts-node')\")/../../esm.mjs --loader ./.pnp.loader.mjs scripts/ls-workspaces.ts",
    "bootstrap": "yarn node --loader file://$(yarn node -p \"require.resolve('ts-node')\")/../../esm.mjs --loader ./.pnp.loader.mjs scripts/bootstrap.ts",
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "build-storybook": "turbo run build-storybook",
    "chromatic": "turbo run chromatic"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@types/node": "18.15.11",
    "@yarnpkg/sdks": "3.0.0-rc.42",
    "arg": "5.0.2",
    "execa": "7.1.1",
    "glob-to-regexp": "0.4.1",
    "ts-node": "10.9.1",
    "turbo": "1.8.8",
    "typescript": "4.9.5"
  },
  "dependenciesMeta": {
    "ts-node@10.9.1": {
      "unplugged": true
    }
  }
}
