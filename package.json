{
  "name": "vite-storybook-boilerplate",
  "private": true,
  "type": "module",
  "packageManager": "yarn@4.2.1",
  "scripts": {
    "postinstall": "husky",
    "g:dotenv": "cd $INIT_CWD && dotenvx",
    "g:dotenv-get": "cd $INIT_CWD && yarn g:dotenv-run -- node -p \"process.env.$@\"",
    "g:dotenv-run": "cd $INIT_CWD && yarn g:dotenv run -q -f $PROJECT_CWD/.env.defaults -f $PROJECT_CWD/.env -f $INIT_CWD/.env.defaults --overload",
    "g:dotenv-vault": "cd $INIT_CWD && dotenv-vault",
    "g:swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
    "g:run-script": "cd $INIT_CWD && yarn g:dotenv-run -- node --loader file://$(yarn g:swc-register-path)",
    "run-script": "yarn g:run-script",
    "turbo": "yarn run-script bin/turbo.ts",
    "ls-workspaces": "yarn run-script bin/ls-workspaces.ts",
    "bootstrap": "yarn workspace stylelint-config turbo run build && yarn run-script bin/bootstrap.ts",
    "apply-versions": "yarn run-script bin/apply-versions.ts",
    "dev": "yarn turbo --background run dev",
    "build": "yarn turbo run build",
    "lint": "yarn g:prettier --check . && yarn g:lint-js",
    "lint-staged": "yarn workspace commons turbo run build && yarn exec lint-staged",
    "g:lint-css": "cd $INIT_CWD && stylelint --ignore-path $PROJECT_CWD/.gitignore \"**/*.{sc,c}ss\"",
    "format": "yarn g:format"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "devDependencies": {
    "@dotenvx/dotenvx": "0.37.1",
    "@swc-node/register": "1.9.0",
    "@swc/core": "1.5.3",
    "@types/glob-to-regexp": "0.4.4",
    "@types/node": "20.12.8",
    "@types/semver": "7.5.8",
    "@yarnpkg/core": "4.0.5",
    "@yarnpkg/fslib": "3.0.2",
    "@yarnpkg/sdks": "3.1.2",
    "arg": "5.0.2",
    "commons": "workspace:*",
    "dotenv-flow": "4.1.0",
    "dotenv-vault": "1.26.1",
    "eslint": "8.57.0",
    "execa": "8.0.1",
    "glob-to-regexp": "0.4.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "playwright": "1.43.1",
    "semver": "7.6.0",
    "stylelint": "16.5.0",
    "turbo": "1.13.3",
    "typescript": "5.4.5",
    "yaml": "2.4.2"
  },
  "resolutions": {
    "@swc-node/register@1.9.0": "patch:@swc-node/register@npm%3A1.9.0#~/.yarn/patches/@swc-node-register-npm-1.9.0-4530442723.patch",
    "dotenv-vault@1.26.1": "patch:dotenv-vault@npm%3A1.26.1#~/.yarn/patches/dotenv-vault-npm-1.26.1-f283958789.patch",
    "dotenv-vault/@oclif/core@^1": "npm:3.23.0",
    "jest-config@npm:^29.7.0": "patch:jest-config@npm%3A29.7.0#~/.yarn/patches/jest-config-npm-29.7.0-97d8544d74.patch"
  }
}
