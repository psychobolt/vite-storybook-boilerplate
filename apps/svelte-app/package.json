{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.3.1",
	"scripts": {
		"dotenv-run": "yarn dotenvx -q run -f ../../.env.defaults -f ../../.env -f .env.defaults --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"ESM_REGISTER=$(yarn swc-register-path)\" -- node --import file://$(yarn node -p \"path.join(process.env.PROJECT_CWD, '../../bin/esm-register.js')\")",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn run-script $(yarn bin eslint) -c eslint.config.ts .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.0.0",
		"@eslint/js": "9.5.0",
		"@fontsource/fira-mono": "5.0.13",
		"@neoconfetti/svelte": "2.2.1",
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/adapter-vercel": "5.3.2",
		"@sveltejs/kit": "2.5.17",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@swc-node/register": "1.9.2",
		"@swc/core": "1.6.5",
		"@types/cookie": "0.6.0",
		"@types/eslint": "8.56.10",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "8.42.3",
		"@types/node": "20.14.8",
		"@typescript-eslint/utils": "7.14.1",
		"commons": "workspace:*",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.41.0",
		"globals": "15.6.0",
		"html-ui": "workspace:*",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.5",
		"sass": "1.77.6",
		"stylelint": "16.6.1",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.1.0",
		"svelte": "4.2.18",
		"svelte-check": "3.8.2",
		"svelte-eslint-parser": "0.39.2",
		"svelte-language-server": "0.16.11",
		"tslib": "2.6.3",
		"turbo": "2.0.5",
		"typescript": "5.4.5",
		"typescript-eslint": "7.14.1",
		"vite": "5.3.1"
	},
	"resolutions": {
		"@dotenvx/dotenvx@0.26.0": "patch:@dotenvx/dotenvx@npm%3A0.26.0#../../.yarn/patches/@dotenvx-dotenvx-npm-0.26.0-f0a78a0617.patch",
		"@swc-node/register@1.9.2": "patch:@swc-node/register@npm%3A1.9.2#../../.yarn/patches/@swc-node-register-npm-1.9.2-365a3de13b.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
