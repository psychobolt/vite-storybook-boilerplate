{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.1.1",
	"scripts": {
		"dotenv-run": "yarn dotenvx -q run -f ../../.env.defaults -f ../../.env -f .env.defaults --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "yarn dotenv-run -- node --loader file://$(yarn swc-register-path)",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn run-script $(yarn bin eslint) -c eslint.config.ts .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier . --write ."
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "0.26.0",
		"@eslint/js": "8.57.0",
		"@fontsource/fira-mono": "5.0.12",
		"@neoconfetti/svelte": "2.2.1",
		"@sveltejs/adapter-auto": "3.2.0",
		"@sveltejs/adapter-vercel": "5.2.0",
		"@sveltejs/kit": "2.5.4",
		"@sveltejs/vite-plugin-svelte": "3.0.2",
		"@swc-node/register": "1.9.0",
		"@swc/core": "1.4.11",
		"@types/cookie": "0.6.0",
		"@types/eslint": "8.56.6",
		"@types/eslint__js": "8.42.3",
		"@types/node": "20.11.30",
		"commons": "workspace:*",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.36.0-next.13",
		"globals": "15.0.0",
		"html-ui": "workspace:*",
		"prettier": "3.2.5",
		"prettier-plugin-svelte": "3.2.2",
		"sass": "1.72.0",
		"stylelint": "16.3.1",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.0.0",
		"svelte": "4.2.12",
		"svelte-check": "3.6.8",
		"svelte-eslint-parser": "0.33.1",
		"svelte-language-server": "0.16.6",
		"tslib": "2.6.2",
		"turbo": "1.13.0",
		"typescript": "5.4.3",
		"typescript-eslint": "7.4.0",
		"vite": "5.2.6"
	},
	"resolutions": {
		"@dotenvx/dotenvx@0.26.0": "patch:@dotenvx/dotenvx@npm%3A0.26.0#../../.yarn/patches/@dotenvx-dotenvx-npm-0.26.0-f0a78a0617.patch",
		"@swc-node/register@1.9.0": "patch:@swc-node/register@npm%3A1.9.0#../../.yarn/patches/@swc-node-register-npm-1.9.0-4530442723.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
