{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.9.2",
	"scripts": {
		"dotenv-run": "yarn dotenvx run -q --ignore=MISSING_ENV_FILE -f ../../.env.defaults -f ../../.env -f .env.defaults -f .env --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"TS_NODE_PROJECT=$INIT_CWD/tsconfig.json\" --env=\"ESM_REGISTER=file://$(yarn swc-register-path)\" -- node --enable-source-maps --import file://$(yarn node -p \"path.join(process.env.PROJECT_CWD, '../../bin/esm-register.js')\")",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn check && eslint --flag v10_config_lookup_from_file .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.48.4",
		"@eslint/js": "9.32.0",
		"@fontsource/fira-mono": "5.2.6",
		"@neoconfetti/svelte": "2.2.2",
		"@sveltejs/adapter-auto": "6.0.1",
		"@sveltejs/adapter-vercel": "5.8.1",
		"@sveltejs/kit": "2.26.1",
		"@sveltejs/vite-plugin-svelte": "6.1.0",
		"@swc-node/register": "1.10.0",
		"@swc/core": "1.6.7",
		"@types/eslint": "9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/node": "22.17.0",
		"@typescript-eslint/utils": "8.38.0",
		"commons": "workspace:*",
		"eslint": "9.32.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.11.0",
		"globals": "16.3.0",
		"html-ui": "workspace:*",
		"jiti": "2.5.1",
		"postcss": "8.5.6",
		"prettier": "3.6.2",
		"prettier-plugin-svelte": "3.4.0",
		"sass": "1.89.2",
		"stylelint": "16.23.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "15.0.1",
		"svelte": "5.37.1",
		"svelte-check": "4.3.0",
		"svelte-eslint-parser": "1.3.0",
		"svelte-language-server": "0.17.17",
		"tslib": "2.8.1",
		"turbo": "2.5.5",
		"typescript": "5.9.2",
		"typescript-eslint": "8.38.0",
		"vite": "7.0.6"
	},
	"resolutions": {
		"@swc-node/register@1.10.0": "patch:@swc-node/register@npm%3A1.10.0#../../.yarn/patches/@swc-node-register-npm-1.10.0-3e68312da5.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
