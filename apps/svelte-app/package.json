{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.2.2",
	"scripts": {
		"dotenv-run": "yarn dotenvx -q run -f ../../.env.defaults -f ../../.env -f .env.defaults --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "yarn dotenv-run -- node --loader file://$(yarn swc-register-path)",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn run-script $(yarn bin eslint) -c eslint.config.ts .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "0.44.1",
		"@eslint/js": "9.4.0",
		"@fontsource/fira-mono": "5.0.13",
		"@neoconfetti/svelte": "2.2.1",
		"@sveltejs/adapter-auto": "3.2.2",
		"@sveltejs/adapter-vercel": "5.3.2",
		"@sveltejs/kit": "2.5.17",
		"@sveltejs/vite-plugin-svelte": "3.1.1",
		"@swc-node/register": "1.9.0",
		"@swc/core": "1.5.3",
		"@types/cookie": "0.6.0",
		"@types/eslint": "8.56.10",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "8.42.3",
		"@types/node": "20.14.7",
		"@typescript-eslint/utils": "7.11.0",
		"commons": "workspace:*",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.39.4",
		"globals": "15.3.0",
		"html-ui": "workspace:*",
		"prettier": "3.3.2",
		"prettier-plugin-svelte": "3.2.4",
		"sass": "1.77.6",
		"stylelint": "16.6.1",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.1.0",
		"svelte": "4.2.18",
		"svelte-check": "3.8.1",
		"svelte-eslint-parser": "0.36.0",
		"svelte-language-server": "0.16.10",
		"tslib": "2.6.2",
		"turbo": "2.0.4",
		"typescript": "5.4.5",
		"typescript-eslint": "7.11.0",
		"vite": "5.2.12"
	},
	"resolutions": {
		"@dotenvx/dotenvx@0.26.0": "patch:@dotenvx/dotenvx@npm%3A0.26.0#../../.yarn/patches/@dotenvx-dotenvx-npm-0.26.0-f0a78a0617.patch",
		"@swc-node/register@1.9.0": "patch:@swc-node/register@npm%3A1.9.0#../../.yarn/patches/@swc-node-register-npm-1.9.0-4530442723.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
