{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.5.2",
	"scripts": {
		"dotenv-run": "yarn dotenvx -q run -f ../../.env.defaults -f ../../.env -f .env.defaults -f .env --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"ESM_REGISTER=file://$(yarn swc-register-path)\" -- node --enable-source-maps --import file://$(yarn node -p \"path.join(process.env.PROJECT_CWD, '../../bin/esm-register.js')\")",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn run-script $(yarn bin eslint) -c eslint.config.ts .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.24.0",
		"@eslint/js": "9.15.0",
		"@fontsource/fira-mono": "5.1.0",
		"@neoconfetti/svelte": "2.2.1",
		"@sveltejs/adapter-auto": "3.3.1",
		"@sveltejs/adapter-vercel": "5.4.8",
		"@sveltejs/kit": "2.8.2",
		"@sveltejs/vite-plugin-svelte": "4.0.1",
		"@swc-node/register": "1.10.0",
		"@swc/core": "1.6.7",
		"@types/eslint": "9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "8.42.3",
		"@types/node": "22.9.3",
		"@typescript-eslint/utils": "8.15.0",
		"commons": "workspace:*",
		"eslint": "9.15.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-svelte": "2.46.0",
		"globals": "15.12.0",
		"html-ui": "workspace:*",
		"postcss": "8.4.49",
		"prettier": "3.3.3",
		"prettier-plugin-svelte": "3.3.2",
		"sass": "1.81.0",
		"stylelint": "16.10.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "13.1.0",
		"svelte": "5.2.7",
		"svelte-check": "4.0.9",
		"svelte-eslint-parser": "0.43.0",
		"svelte-language-server": "0.17.7",
		"tslib": "2.8.1",
		"turbo": "2.3.1",
		"typescript": "5.6.3",
		"typescript-eslint": "8.15.0",
		"vite": "5.4.11"
	},
	"resolutions": {
		"@swc-node/register@1.10.0": "patch:@swc-node/register@npm%3A1.10.0#../../.yarn/patches/@swc-node-register-npm-1.10.0-3e68312da5.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
