{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "yarn vite build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "yarn check --watch",
		"dev": "yarn vite dev --open",
		"dotenv-run": "yarn dotenvx run -q --ignore=MISSING_ENV_FILE -f ../../.env.defaults -f ../../.env -f .env.defaults -f .env --overload",
		"format": "yarn prettier --write $(echo \"${@:-.}\")",
		"lint": "yarn prettier --check . && stylelint **/*.{sc,c}ss && yarn check && node --experimental-import-meta-resolve ../../packages/commons/esm/bin/lint.js --runner eslint",
		"prettier": "prettier --ignore-path ../../.gitignore --ignore-path ../../packages/commons/.prettierignore --ignore-path ./.gitignore --ignore-path ./.prettierignore",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"TS_NODE_PROJECT=$INIT_CWD/tsconfig.json\" -- node --enable-source-maps --experimental-transform-types --disable-warning=ExperimentalWarning -r $(yarn swc-register-path)",
		"start": "yarn vite preview",
		"swc-register-path": "yarn node -p \"require.resolve('@swc-node/register')\"",
		"vite": "yarn run-script $(yarn bin vite)"
	},
	"resolutions": {
		"@swc-node/register@1.10.0": "patch:@swc-node/register@npm%3A1.10.0#../../.yarn/patches/@swc-node-register-npm-1.10.0-3e68312da5.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui",
		"stylelint@npm:17.0.0": "patch:stylelint@npm%3A17.0.0#../../.yarn/patches/stylelint-npm-17.0.0-2a27df4ec4.patch"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.52.0",
		"@eslint/js": "9.39.2",
		"@fontsource/fira-mono": "5.2.7",
		"@neoconfetti/svelte": "2.2.2",
		"@sveltejs/adapter-auto": "7.0.0",
		"@sveltejs/adapter-vercel": "6.3.1",
		"@sveltejs/kit": "2.50.1",
		"@sveltejs/vite-plugin-svelte": "6.2.4",
		"@swc-node/register": "1.11.1",
		"@swc/core": "1.15.11",
		"@types/eslint": "9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/node": "24.10.9",
		"@typescript-eslint/utils": "8.54.0",
		"arg": "5.0.2",
		"commons": "workspace:*",
		"eslint": "9.39.2",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.14.0",
		"globals": "17.2.0",
		"html-ui": "workspace:*",
		"jiti": "2.6.1",
		"postcss": "8.5.6",
		"prettier": "3.8.1",
		"prettier-plugin-packagejson": "3.0.0",
		"prettier-plugin-sh": "0.18.0",
		"prettier-plugin-svelte": "3.4.1",
		"sass": "1.97.3",
		"stylelint": "17.1.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "17.0.0",
		"svelte": "5.49.1",
		"svelte-check": "4.3.6",
		"svelte-eslint-parser": "1.4.1",
		"svelte-language-server": "0.17.24",
		"tslib": "2.8.1",
		"turbo": "2.8.1",
		"typescript": "5.9.3",
		"typescript-eslint": "8.54.0",
		"vite": "7.3.1"
	},
	"packageManager": "yarn@4.12.0"
}
