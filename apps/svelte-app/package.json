{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.6.0",
	"scripts": {
		"dotenv-run": "yarn dotenvx -q run -f ../../.env.defaults -f ../../.env -f .env.defaults -f .env --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"TS_NODE_PROJECT=$INIT_CWD/tsconfig.json\" --env=\"ESM_REGISTER=file://$(yarn swc-register-path)\" -- node --enable-source-maps --import file://$(yarn node -p \"path.join(process.env.PROJECT_CWD, '../../bin/esm-register.js')\")",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn check && yarn run-script $(yarn bin eslint) -c eslint.config.ts .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.37.0",
		"@eslint/js": "9.20.0",
		"@fontsource/fira-mono": "5.1.1",
		"@neoconfetti/svelte": "2.2.1",
		"@sveltejs/adapter-auto": "4.0.0",
		"@sveltejs/adapter-vercel": "5.6.3",
		"@sveltejs/kit": "2.17.2",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@swc-node/register": "1.10.0",
		"@swc/core": "1.6.7",
		"@types/eslint": "9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/eslint__js": "9.14.0",
		"@types/node": "22.13.4",
		"@typescript-eslint/utils": "8.24.1",
		"commons": "workspace:*",
		"eslint": "9.20.1",
		"eslint-config-prettier": "10.0.1",
		"eslint-plugin-svelte": "2.46.1",
		"globals": "16.0.0",
		"html-ui": "workspace:*",
		"jiti": "2.4.2",
		"postcss": "8.5.3",
		"prettier": "3.5.1",
		"prettier-plugin-svelte": "3.3.3",
		"sass": "1.85.0",
		"stylelint": "16.14.1",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "14.0.0",
		"svelte": "5.20.2",
		"svelte-check": "4.1.4",
		"svelte-eslint-parser": "0.43.0",
		"svelte-language-server": "0.17.10",
		"tslib": "2.8.1",
		"turbo": "2.4.2",
		"typescript": "5.7.3",
		"typescript-eslint": "8.24.1",
		"vite": "6.1.1"
	},
	"resolutions": {
		"@swc-node/register@1.10.0": "patch:@swc-node/register@npm%3A1.10.0#../../.yarn/patches/@swc-node-register-npm-1.10.0-3e68312da5.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
