{
	"name": "svelte-app",
	"private": true,
	"type": "module",
	"packageManager": "yarn@4.9.1",
	"scripts": {
		"dotenv-run": "yarn dotenvx run -q --ignore=MISSING_ENV_FILE -f ../../.env.defaults -f ../../.env -f .env.defaults -f .env --overload",
		"swc-register-path": "yarn node -p \"path.join(require.resolve('@swc-node/register'), '../esm/esm.mjs')\"",
		"run-script": "cd $INIT_CWD && yarn dotenv-run --env=\"TS_NODE_PROJECT=$INIT_CWD/tsconfig.json\" --env=\"ESM_REGISTER=file://$(yarn swc-register-path)\" -- node --enable-source-maps --import file://$(yarn node -p \"path.join(process.env.PROJECT_CWD, '../../bin/esm-register.js')\")",
		"dev": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint-js": "yarn check && eslint --flag unstable_config_lookup_from_file .",
		"lint": "prettier --check . && stylelint **/*.{sc,c}ss && yarn lint-js",
		"format": "prettier --write $(echo \"${@:-.}\")"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.44.1",
		"@eslint/js": "9.28.0",
		"@fontsource/fira-mono": "5.2.6",
		"@neoconfetti/svelte": "2.2.2",
		"@sveltejs/adapter-auto": "6.0.1",
		"@sveltejs/adapter-vercel": "5.7.2",
		"@sveltejs/kit": "2.21.1",
		"@sveltejs/vite-plugin-svelte": "5.0.3",
		"@swc-node/register": "1.10.0",
		"@swc/core": "1.6.7",
		"@types/eslint": "9.6.1",
		"@types/eslint-config-prettier": "6.11.3",
		"@types/node": "22.15.29",
		"@typescript-eslint/utils": "8.33.0",
		"commons": "workspace:*",
		"eslint": "9.28.0",
		"eslint-config-prettier": "10.1.5",
		"eslint-plugin-svelte": "3.9.0",
		"globals": "16.2.0",
		"html-ui": "workspace:*",
		"jiti": "2.4.2",
		"postcss": "8.5.4",
		"prettier": "3.5.3",
		"prettier-plugin-svelte": "3.4.0",
		"sass": "1.89.0",
		"stylelint": "16.20.0",
		"stylelint-config-prettier-scss": "1.0.0",
		"stylelint-config-standard-scss": "15.0.1",
		"svelte": "5.33.10",
		"svelte-check": "4.2.1",
		"svelte-eslint-parser": "1.2.0",
		"svelte-language-server": "0.17.15",
		"tslib": "2.8.1",
		"turbo": "2.5.4",
		"typescript": "5.8.3",
		"typescript-eslint": "8.33.0",
		"vite": "6.3.5"
	},
	"resolutions": {
		"@swc-node/register@1.10.0": "patch:@swc-node/register@npm%3A1.10.0#../../.yarn/patches/@swc-node-register-npm-1.10.0-3e68312da5.patch",
		"commons": "portal:../../packages/commons",
		"html-ui": "portal:../../packages/html-ui"
	}
}
