{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:best-practices"],
  "baseBranches": ["$default", "bitbucket"],
  "useBaseBranchConfig": "merge",
  "automerge": true,
  "packageRules": [
    {
      "matchPackageNames": ["stylelint-config-standard-scss"],
      "allowedVersions": "<13"
    }
  ]
}
