    "stylelint-order": "7.0.0"
  },
  "devDependencies": {
    "arg": "5.0.2",
    "commons": "workspace:*",
    "eslint": "9.36.0",
    "stylelint": "16.24.0",
    "turbo": "2.5.6",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "stylelint": "*"
