{
  "name": "commons",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn g:bootstrap",
    "build": "yarn build-esm && yarn build-cjs",
    "build-esm": "rimraf esm/ && tsc --project tsconfig.esm.json && tsc --project tsconfig.dts.json",
    "build-cjs": "rimraf cjs/ && tsc --project tsconfig.cjs.json && tsconfig-to-dual-package tsconfig.cjs.json",
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:test-storybook-path": "yarn bin test-storybook",
    "g:test-storybook": "cd $INIT_CWD && yarn g:ts-script $(yarn g:test-storybook-path)",
    "lint": "yarn g:prettier --check . && yarn g:lint-js",
    "format": "yarn g:prettier --write ."
  },
  "dependencies": {
    "@eslint/eslintrc": "2.1.3",
    "@jest/types": "29.6.3",
    "@storybook/addon-coverage": "1.0.0",
    "@storybook/addon-essentials": "7.6.0-alpha.6",
    "@storybook/addon-interactions": "7.6.0-alpha.6",
    "@storybook/addon-links": "7.6.0-alpha.6",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/builder-vite": "7.6.0-alpha.6",
    "@storybook/test-runner": "0.16.0",
    "@storybook/types": "7.6.0-alpha.6",
    "@tsconfig/recommended": "1.0.3",
    "@types/eslint": "8.44.7",
    "@typescript-eslint/eslint-plugin": "6.13.0",
    "@typescript-eslint/parser": "6.13.0",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-config-standard-with-typescript": "40.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-mdx": "2.2.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "playwright": "1.40.0",
    "playwright-chromium": "1.40.0",
    "postcss": "8.4.31",
    "postcss-scss": "4.0.9",
    "prettier": "3.1.0",
    "prettier-config-standard": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "stylelint": "15.11.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "11.1.0",
    "tsconfig-to-dual-package": "1.2.0",
    "vite": "5.0.3",
    "vite-plugin-turbosnap": "1.0.3",
    "vite-tsconfig-paths": "4.2.1"
  },
  "devDependencies": {
    "turbo": "1.10.16",
    "typescript": "5.2.2"
  }
}
