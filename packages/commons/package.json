{
  "name": "commons",
  "private": true,
  "type": "module",
  "bin": {
    "highlight": "esm/bin/highlight.js",
    "inventory": "esm/bin/inventory.js",
    "lint": "esm/bin/lint.js"
  },
  "scripts": {
    "build": "yarn build-esm && yarn build-cjs",
    "build-cjs": "rimraf cjs/ && tsc --project tsconfig.cjs.json && tsconfig-to-dual-package tsconfig.cjs.json",
    "build-esm": "rimraf esm/ && tsc --project tsconfig.esm.json",
    "format": "yarn g:prettier --write --config esm/prettier.config.js $(echo \"${@:-.}\")",
    "g:build-inventory": "cd $INIT_CWD && yarn g:run-script $PROJECT_CWD/packages/commons/esm/bin/inventory.js",
    "g:chromatic": "cd $INIT_CWD && yarn g:dotenv-run -- chromatic",
    "g:format": "cd $INIT_CWD && yarn g:prettier --write $(echo \"${@:-.}\")",
    "g:lint": "cd $INIT_CWD && yarn tsc --noEmit && lint",
    "g:lint-staged": "cd $INIT_CWD && lint-staged",
    "g:prettier": "cd $INIT_CWD && prettier --ignore-path $PROJECT_CWD/.gitignore --ignore-path $PROJECT_CWD/packages/commons/.prettierignore --ignore-path $INIT_CWD/.gitignore --ignore-path ./.prettierignore",
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:storybook-path)",
    "g:storybook-path": "yarn bin storybook",
    "g:test-storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:test-storybook-path)",
    "g:test-storybook-path": "yarn bin test-storybook",
    "g:vitest": "cd $INIT_CWD && yarn g:run-script $(yarn bin vitest)",
    "lint": "yarn g:prettier --check --config esm/prettier.config.js . && yarn g:lint"
  },
  "dependencies": {
    "@chromatic-com/storybook": "4.1.3",
    "@eslint/core": "1.0.1",
    "@jest/types": "29.6.3",
    "@storybook/builder-vite": "9.1.17",
    "@storybook/test-runner": "0.23.0",
    "@types/eslint": "9.6.1",
    "@types/lodash": "4",
    "autoprefixer": "10.4.23",
    "color": "5.0.3",
    "eslint-plugin-mdx": "3.6.2",
    "eslint-plugin-storybook": "9.1.17",
    "jiti": "2.6.1",
    "lint-staged": "16.2.7",
    "lodash": "4.17.21",
    "neostandard": "0.12.2",
    "postcss": "8.5.6",
    "postcss-pseudo-classes": "0.4.0",
    "postcss-scss": "4.0.9",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-syntax-highlighter": "16.1.0",
    "sharp": "0.34.5",
    "storybook-zeplin": "3.0.0",
    "ts-enum-util": "4.1.0",
    "tsconfck": "3.1.6",
    "vite-tsconfig-paths": "5.1.4"
  },
  "devDependencies": {
    "@storybook/addon-docs": "9.1.17",
    "@storybook/addon-links": "9.1.17",
    "@storybook/addon-onboarding": "9.1.17",
    "@storybook/addon-vitest": "9.1.17",
    "@storybook/global": "5.0.0",
    "@types/react-syntax-highlighter": "15.5.13",
    "arg": "5.0.2",
    "chromatic": "13.3.5",
    "eslint": "9.39.2",
    "jest-junit": "16.0.0",
    "playwright": "1.57.0",
    "playwright-chromium": "1.57.0",
    "prettier": "3.7.4",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-packagejson": "2.5.21",
    "prettier-plugin-sh": "0.18.0",
    "rimraf": "6.1.2",
    "storybook": "9.1.17",
    "stylelint": "16.26.1",
    "tsconfig-to-dual-package": "1.2.0",
    "turbo": "2.7.4",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vitest": "3.2.4"
  }
}
