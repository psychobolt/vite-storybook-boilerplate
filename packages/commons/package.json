{
  "name": "commons",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "bootstrap": "yarn g:bootstrap",
    "build": "yarn build-esm && yarn build-cjs",
    "build-esm": "rimraf esm/ && tsc && tsc --project tsconfig.dts.json",
    "build-cjs": "rimraf cjs/ && tsc --project tsconfig.cjs.json && tsconfig-to-dual-package tsconfig.cjs.json",
    "lint": "yarn g:prettier --check . && yarn g:lint-js",
    "format": "yarn g:prettier --write ."
  },
  "dependencies": {
    "@eslint/eslintrc": "2.0.3",
    "@storybook/addon-coverage": "0.0.8",
    "@storybook/addon-essentials": "7.0.15",
    "@storybook/addon-interactions": "7.0.15",
    "@storybook/addon-links": "7.0.15",
    "@storybook/builder-vite": "7.0.15",
    "@storybook/types": "7.0.15",
    "@tsconfig/recommended": "1.0.2",
    "@types/eslint": "8.40.0",
    "@types/node": "18.16.14",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-config-standard-with-typescript": "34.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-mdx": "2.1.0",
    "eslint-plugin-n": "16.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "postcss": "8.4.23",
    "postcss-scss": "4.0.6",
    "prettier": "2.8.8",
    "prettier-config-standard": "5.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.1",
    "stylelint": "15.6.2",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "9.0.0",
    "tsconfig-to-dual-package": "1.2.0",
    "typescript": "5.0.4",
    "vite": "4.3.8",
    "vite-plugin-turbosnap": "1.0.2"
  },
  "devDependencies": {
    "turbo": "1.9.8"
  }
}
