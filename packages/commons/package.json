{
  "name": "commons",
  "private": true,
  "type": "module",
  "exports": {
    "./*.json": "./*.json",
    "./esm/*": "./esm/*.js",
    "./esm/.storybook/*": "./esm/.storybook/*.js",
    "./esm/.storybook/addons/*": "./esm/.storybook/addons/*.js",
    "./esm/.storybook/utils/*": "./esm/.storybook/utils/*.js",
    "./cjs/*.cjs": "./cjs/*.cjs"
  },
  "typesVersions": {
    "*": {
      "./esm/*": [
        "./esm/*.d.ts"
      ],
      "./esm/.storybook/*": [
        "./esm/.storybook/*.d.ts"
      ],
      "./esm/.storybook/addons/*": [
        "./esm/.storybook/addons/*.d.ts"
      ],
      "./esm/.storybook/utils/*": [
        "./esm/.storybook/utils/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "yarn build-esm && yarn build-cjs",
    "build-esm": "rimraf esm/ && tsc --project tsconfig.esm.json",
    "build-cjs": "rimraf cjs/ && tsc --project tsconfig.cjs.json && tsconfig-to-dual-package tsconfig.cjs.json",
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:storybook-path": "yarn bin storybook",
    "g:storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:storybook-path)",
    "g:test-storybook-path": "yarn bin test-storybook",
    "g:test-storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:test-storybook-path)",
    "g:vitest": "cd $INIT_CWD && yarn g:run-script $(yarn bin vitest)",
    "g:prettier": "cd $INIT_CWD && prettier --ignore-path $PROJECT_CWD/.gitignore --ignore-path $PROJECT_CWD/packages/commons/.prettierignore --ignore-path $INIT_CWD/.gitignore --ignore-path ./.prettierignore",
    "g:eslint-path": "yarn bin eslint",
    "g:lint-js": "cd $INIT_CWD && yarn tsc --noEmit && eslint --flag unstable_config_lookup_from_file .",
    "lint": "yarn g:prettier --check --config esm/prettier.config.js . && yarn g:lint-js",
    "g:format": "cd $INIT_CWD && yarn g:prettier --write $(echo \"${@:-.}\")",
    "format": "yarn g:prettier --write --config esm/prettier.config.js $(echo \"${@:-.}\")"
  },
  "dependencies": {
    "@chromatic-com/storybook": "4.0.1",
    "@jest/types": "29.6.3",
    "@storybook/addon-docs": "9.0.13",
    "@storybook/builder-vite": "9.0.13",
    "@storybook/test-runner": "0.23.0",
    "@types/eslint": "9.6.1",
    "@types/lodash": "4",
    "autoprefixer": "10.4.21",
    "eslint": "9.29.0",
    "eslint-plugin-mdx": "3.5.0",
    "jiti": "2.4.2",
    "lodash": "4.17.21",
    "neostandard": "0.12.1",
    "postcss": "8.5.6",
    "postcss-pseudo-classes": "0.4.0",
    "postcss-scss": "4.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-syntax-highlighter": "15.6.1",
    "storybook-zeplin": "3.0.0",
    "ts-enum-util": "4.1.0",
    "tsconfck": "3.1.6",
    "vite-tsconfig-paths": "5.1.4"
  },
  "devDependencies": {
    "@storybook/addon-links": "9.0.13",
    "@storybook/addon-onboarding": "9.0.13",
    "@storybook/addon-vitest": "9.0.13",
    "@types/lint-staged": "13.3.0",
    "@types/react-syntax-highlighter": "15.5.13",
    "jest-junit": "16.0.0",
    "playwright": "1.53.1",
    "playwright-chromium": "1.53.1",
    "prettier": "3.6.1",
    "prettier-config-standard": "7.0.0",
    "prettier-plugin-sh": "0.18.0",
    "rimraf": "6.0.1",
    "storybook": "9.0.13",
    "tsconfig-to-dual-package": "1.2.0",
    "turbo": "2.5.4",
    "typescript": "5.8.3",
    "vite": "7.0.0",
    "vitest": "3.2.4"
  }
}
