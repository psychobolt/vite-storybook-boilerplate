{
  "name": "commons",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "yarn build-esm && yarn build-cjs",
    "build-esm": "rimraf esm/ && tsc --project tsconfig.esm.json && tsc --project tsconfig.dts.json",
    "build-cjs": "rimraf cjs/ && tsc --project tsconfig.cjs.json && tsconfig-to-dual-package tsconfig.cjs.json",
    "g:rimraf": "cd $INIT_CWD && rimraf",
    "g:storybook-path": "yarn bin storybook",
    "g:storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:storybook-path)",
    "g:test-storybook-path": "yarn bin test-storybook",
    "g:test-storybook": "cd $INIT_CWD && yarn g:run-script $(yarn g:test-storybook-path)",
    "g:prettier": "cd $INIT_CWD && prettier --ignore-path $PROJECT_CWD/.gitignore --ignore-path ./.prettierignore",
    "lint": "yarn g:prettier --check . && yarn g:lint-js",
    "format": "yarn g:prettier --write ."
  },
  "dependencies": {
    "@eslint/eslintrc": "3.0.2",
    "@jest/types": "29.6.3",
    "@storybook/addon-coverage": "1.0.1",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/addon-onboarding": "1.0.11",
    "@storybook/builder-vite": "7.6.17",
    "@storybook/test-runner": "0.16.0",
    "@storybook/types": "7.6.17",
    "@tsconfig/recommended": "1.0.3",
    "@types/eslint": "8.56.5",
    "@types/lodash": "4",
    "@typescript-eslint/eslint-plugin": "7.1.1",
    "@typescript-eslint/parser": "7.1.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-config-standard-jsx": "11.0.0",
    "eslint-config-standard-react": "13.0.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-mdx": "3.1.5",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.34.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "jest-junit": "16.0.0",
    "lodash": "4.17.21",
    "playwright": "1.42.1",
    "playwright-chromium": "1.42.1",
    "postcss": "8.4.35",
    "postcss-scss": "4.0.9",
    "prettier": "3.2.5",
    "prettier-config-standard": "7.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rimraf": "5.0.5",
    "storybook": "7.6.17",
    "stylelint": "15.11.0",
    "stylelint-config-prettier-scss": "1.0.0",
    "stylelint-config-standard-scss": "12.0.0",
    "ts-enum-util": "4.0.2",
    "vite": "5.1.5",
    "vite-plugin-turbosnap": "1.0.3",
    "vite-tsconfig-paths": "4.3.1"
  },
  "devDependencies": {
    "tsconfig-to-dual-package": "1.2.0",
    "turbo": "1.12.4",
    "typescript": "5.3.3"
  }
}
