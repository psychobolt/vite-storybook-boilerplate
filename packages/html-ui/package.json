{
  "name": "html-ui",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./*": "./dist/*"
  },
  "scripts": {
    "build": "vite build",
    "build-storybook": "yarn g:storybook build --stats-json",
    "chromatic": "yarn g:chromatic",
    "coverage": "yarn g:rimraf coverage/ && yarn test --coverage",
    "dev": "yarn watch & yarn g:storybook dev -p $(yarn g:dotenv-get PORT)",
    "format": "yarn g:format",
    "lcov": "nyc report --reporter=lcov -t coverage/storybook --report-dir coverage/storybook",
    "lint": "yarn g:prettier --check . && yarn g:lint --runner stylelint --runner eslint",
    "prepack": "yarn turbo run build",
    "start": "yarn g:dotenv-run -- http-server storybook-static",
    "test": "yarn g:test-storybook --index-json",
    "watch": "yarn build -w"
  },
  "devDependencies": {
    "@open-wc/lit-helpers": "0.7.0",
    "@storybook/addon-coverage": "3.0.0",
    "@storybook/addon-docs": "10.2.10",
    "@storybook/web-components-vite": "10.2.10",
    "@types/react-syntax-highlighter": "15.5.13",
    "classnames": "2.5.1",
    "commons": "workspace:*",
    "eslint": "9.39.3",
    "glob": "13.0.6",
    "http-server": "14.1.1",
    "lit": "3.3.2",
    "nyc": "17.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-syntax-highlighter": "16.1.0",
    "rollup-plugin-no-emit": "1.3.0",
    "sass": "1.97.3",
    "storybook": "10.2.10",
    "stylelint": "17.3.0",
    "stylelint-config": "workspace:*",
    "turbo": "2.8.10",
    "typescript": "5.9.3",
    "typescript-plugin-css-modules": "5.2.0",
    "vite": "7.3.1",
    "vite-plugin-sass-glob-import": "6.0.2"
  },
  "packageManager": "yarn@4.12.0"
}
