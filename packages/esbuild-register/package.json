{
  "name": "esbuild-register",
  "description": "Transpile JSX, TypeScript and esnext features on the fly with esbuild",
  "version": "0.0.0",
  "type": "module",
  "main": "register.cjs",
  "license": "MIT",
  "files": [
    "dist",
    "/register.cjs",
    "/loader.cjs"
  ],
  "exports": {
    ".": "./register.cjs",
    "./loader": "./loader.cjs",
    "./dist/node": "./dist/node.cjs",
    "./dist/*": "./dist/*"
  },
  "peerDependencies": {
    "esbuild": ">=0.12 <1"
  },
  "dependencies": {
    "debug": "^4.3.4"
  }
}
